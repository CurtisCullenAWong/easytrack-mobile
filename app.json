{
  "expo": {
    "name": "EasyTrack",
    "slug": "easytrack",
    "version": "1.0.0",
    "scheme": "easytrack",
    "owner": "thewalkingdev_numoa",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#F1F8E9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.curtiscullenwong.EasyTrack",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "location",
          "location",
          "location",
          "location"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs location access to track you in the background.",
        "NSLocationWhenInUseUsageDescription": "We use your location to show your position on the map."
      }
    },
    "android": {
      "package": "com.curtiscullenwong.EasyTrack",
      "adaptiveIcon": {
        "backgroundColor": "#F1F8E9"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDFmfy3j09egUbTeDImVNnMCFgOjVvLUUM"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "66476e21-4cda-45a9-bfae-9fa38aa3a257"
      }
    },
    "plugins": [
      [
        "expo-updates",
        {
          "username": "thewalkingdev_numoa"
        }
      ],
      "expo-font",
      "expo-asset",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for delivery tracking and real-time updates.",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location in the background for continuous delivery tracking.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for delivery tracking and navigation."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/66476e21-4cda-45a9-bfae-9fa38aa3a257"
    },
    "newArchEnabled": true
  }
}
